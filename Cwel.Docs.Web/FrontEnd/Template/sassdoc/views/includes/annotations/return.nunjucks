{% if item.return %}
<tr>
    <th>
        <h4 class="sd-meta__heading">Returns</h4>
    </th>
    <td>
        <div class="sd-return">
            {% if item.return.type == '*' %}
                Any type
            {% else %}
<code>{{ item.return.type }}</code>
            {% endif %}
            {% if item.return.description %} &mdash; {{ item.return.description|safe }}{% endif %}
        </div>
    </td>
</tr>
{% endif %}
